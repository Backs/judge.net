@model Judge.Application.ViewModels.Contests.ContestTasks.ContestTasksViewModel
@{
    ViewBag.Title = Model.ContestName;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div class="container">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>Label</th>
            <th>Name</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var task in Model.Tasks)
        {
            <tr>
                <td>@task.Label</td>
                <td>@task.Name</td>
            </tr>
        }
        </tbody>
    </table>
</div>